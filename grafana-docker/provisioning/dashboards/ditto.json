          "refId": "mhh",
          "thingID": "org.acme:my-device-2"
        }
      ],
      "title": "DT Location",
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "delimiter": ",",
            "format": "json",
            "jsonPaths": [
              {
                "alias": "raw_lat",
                "path": "/properties/referencePosition/latitude"
              },
              {
                "alias": "raw_lon",
                "path": "/properties/referencePosition/longitude"
              }
            ],
            "source": "Value"
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "latitude",
            "binary": {
              "left": {
                "matcher": {
                  "id": "byName",
                  "options": "raw_lat"
                }
              },
              "operator": "/",
              "right": {
                "fixed": "10000000"
              }
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "longitude",
            "binary": {
              "left": {
                "matcher": {
                  "id": "byName",
                  "options": "raw_lon"
                }
              },
              "operator": "/",
              "right": {
                "fixed": "10000000"
              }
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "disabled": true,
          "id": "limit",
          "options": {
            "limitField": "1"
          }
        }
      ],
      "type": "geomap"
    },
