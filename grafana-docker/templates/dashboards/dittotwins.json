                }
              ],
              "keepTime": false,
              "replace": false,
              "source": "Value"
            }
          },
          {
            "id": "calculateField",
            "options": {
              "alias": "final_latitude",
              "binary": {
                "left": "lat",
                "operator": "/",
                "right": "10000000"
              },
              "mode": "binary",
              "reduce": {
                "reducer": "sum"
              }
            }
          },
          {
            "id": "calculateField",
            "options": {
              "alias": "final_longitude",
              "binary": {
                "left": "lon",
                "operator": "/",
                "right": "10000000"
              },
              "mode": "binary",
              "reduce": {
                "reducer": "sum"
              }
            }
          },
          {
            "id": "limit",
            "options": {
              "limitField": 1
            }
          }
        ],
